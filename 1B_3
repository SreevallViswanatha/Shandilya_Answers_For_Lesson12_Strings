// Write a program in C to find the length of a string without using library function.
#include <stdio.h>
#include <stdlib.h>
int main()
{
    char string[1000];
    int i=0;
    puts("Enter a string:");
    gets(string);
    while(string[i]!='\0')
    {
        i++;
    }
    printf("The length of the string is:%d", i);
}
