// Write a program in C to print individual characters of string in reverse order.
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
    char string[1000];
    int length;
    puts("Enter a string:");
    gets(string);
    length=strlen(string)-1;
    for(; length>=0; length--)
    {
        printf("%c    ", string[length]);
    }
}
