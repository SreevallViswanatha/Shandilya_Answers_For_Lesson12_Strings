// Write a program in C to separate the individual characters from a string.
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
    char string[1000];
    int i, length;
    puts("Enter a string:");
    gets(string);
    length=strlen(string)-1;
    for(i=0; i<=length; i++)
    {
        printf("%c\n", string[i]);
    }
}
