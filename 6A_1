// Write a program in C to Concatenate Two Strings Manually.
#include <stdio.h>
#include <stdlib.h>
int main()
{
    char string1[10000], string2[10000], string3[10000];
    int i, j;
    puts("Enter string 1:");
    gets(string1);
    puts("Enter string 2:");
    gets(string2);
    for(i=0; string1[i]!='\0'; i++)
    {
        string3[i]=string1[i];
    }
    string3[i]=' ';
    i+=1;
    for(j=0; string2[j]!='\0'; j++)
    {
        string3[i]=string2[j];
        i++;
    }
    puts(string3);
}
